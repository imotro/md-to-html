{
    "/> (.+)/g": "<blockquote>$1</blockquote>"
    "/###### (.+)/g": "<h6>$1</h6>",
    "/##### (.+)/g": "<h5>$1</h5>",
    "/#### (.+)/g": "<h4>$1</h4>",
    "/### (.+)/g": "<h3>$1</h3>",
    "/## (.+)/g": "<h2>$1</h2>",
    "/# (.+)/g": "<h1>$1</h1>",
    "/- (.+)/g": "<li>$1</li>",
    "/\*\*(.+?)\*\*/g": "<strong>$1</strong>",
    "/\*(.+?)\*/g": '<em>$1</em>",
    "/~~(.+?)~~/g": '<del>$1</del>",
    "/`(.+?)`/g": '<code>$1</code>",
    "/``(.+?)``/g": '<code>$1</code>",
    "/```(.+?)```/g": '<code>$1</code>",
    "/\[([^\]]+)\]\(([^\)]+)\)/g": "<a href="$2">$1</a>"
}
